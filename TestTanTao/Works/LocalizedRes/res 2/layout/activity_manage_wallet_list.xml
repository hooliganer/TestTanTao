<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context=".ManageWalletList">

    <android.support.design.widget.AppBarLayout
        android:id="@+id/manage_list_appbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/ThemeOverlay.AppCompat.ActionBar">
<android.support.v7.widget.Toolbar
    android:id="@+id/manage_list_toolbar"
    android:layout_height="?attr/actionBarSize"
    android:layout_width="match_parent"
    android:theme="@style/Home_action_menu"
    app:titleMarginStart="0dp"
    app:contentInsetStartWithNavigation="0dp"
    app:title="Test"
    />
    </android.support.design.widget.AppBarLayout>

<android.support.v7.widget.RecyclerView
    android:id="@+id/manage_list_recyclerView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_below="@id/manage_list_appbar"
    android:layout_marginTop="@dimen/margin_l"
    />

</RelativeLayout>