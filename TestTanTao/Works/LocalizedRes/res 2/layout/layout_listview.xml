<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/relative_list"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <LinearLayout
        android:id="@+id/layout_list1"
        android:layout_width="740px"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/M1"
        android:layout_marginTop="@dimen/M2"
        android:layout_toLeftOf="@+id/layout_list2"
        android:layout_marginBottom="@dimen/margin"
        style="@style/htcListItem"
        android:orientation="vertical">

        <TextView
            android:id="@+id/text_date"
            android:layout_width="740px"
            android:layout_height="match_parent"
            style="@style/list_primary_m"
            android:text="2018/12/25 12:30:03"/>
        <TextView
            android:id="@+id/sendReveive"
            android:layout_width="740px"
            android:layout_height="match_parent"
            android:singleLine="true"
            android:layout_marginTop="@dimen/M5"
            android:layout_gravity="left"
            style="@style/list_secondary_m"
            android:text="Reveive from:adji...iw12212e1551"/>
    </LinearLayout>

    <RelativeLayout
        android:id="@+id/layout_list2"
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="@dimen/M5"
        android:layout_marginRight="@dimen/M1"
        android:layout_centerVertical="true"
        android:orientation="vertical">

        <TextView
            android:id="@+id/text_rate"
            style="@style/list_primary_m"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toLeftOf="@+id/wallet_type"
            android:singleLine="true"
            android:textSize="66px"
            android:text="+10.12" />

        <TextView
            android:id="@+id/wallet_type"
            style="@style/list_primary_m"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginLeft="@dimen/M5"
            android:textSize="66px"
            android:text="ETH" />
    </RelativeLayout>


